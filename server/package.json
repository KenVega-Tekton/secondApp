  "license": "MIT",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "concurrently": "3.5.1",
    "express": "4.16.2",
    "jsonwebtoken": "8.1.1",
    "mongoose": "5.0.1"
  }
}
